#
# Personal SOCKS proxy (when connected to insecure networks)
#
Host proxy
	HostName home.kartlytics.com
	DynamicForward localhost:5055
	ServerAliveInterval 30

# sharptooth on VPN (wired)
Host sharptooth
	Hostname sharptooth.kartlytics.com
	ForwardAgent yes

# sharptooth on VPN (wifi)
Host sharptooth-wifi
	Hostname sharptooth2.kartlytics.com
	ForwardAgent yes

# SmartOS GZ
Host wendell.gz
	HostName 192.168.10.128

# Dev zone in SmartOS VM
Host wendell.dev
	HostName 192.168.10.129

# 2019 SmartOS GZ (originally created for Stripe interview)
Host stampy.gz
	HostName 192.168.10.135
# 2019 SmartOS dev zone (originally created for Stripe interview)
Host stampy.dev
	HostName 192.168.10.136

# JOD Ubuntu 14.04.3 VM
Host jod-ubuntu
	HostName 192.168.10.131

